{
  "name": "aztec-crypto-js",
  "version": "0.1.1",
  "description": "AZTEC javascript cryptography library",
  "main": "index.js",
  "engines": {
    "npm": ">=4"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {
    "bn.js": "^4.11.8",
    "elliptic": "^6.4.1",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-util": "^6.0.0",
    "web3-eth-abi": "^1.0.0-beta.36",
    "web3-eth-accounts": "^1.0.0-beta.36",
    "web3-utils": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "braintree-jsdoc-template": "^3.3.0",
