{
  "name": "aztec",
  "version": "0.1.1",
  "description": "AZTEC Smart Contract Repository",
  "main": "truffle-config.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {
    "aztec-crypto-js": "file:aztec-crypto-js",
    "aztec-demo-js": "file:aztec-demo-js",
    "bn.js": "^4.11.8",
    "elliptic": "^6.4.1",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-wallet": "^0.6.3",
    "ganache-cli": "^6.2.3",
    "request-promise": "^4.2.2",
    "truffle": "^5.0.0",
    "truffle-hdwallet-provider": "^1.0.0-web3one.5",
    "web3": "^1.0.0-beta.36",
    "web3-eth-abi": "^1.0.0-beta.36",
    "web3-eth-accounts": "^1.0.0-beta.36",
    "web3-utils": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "braintree-jsdoc-template": "^3.3.0",
    "chai": "^4.2.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-util": "^6.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.2.2",
    "solidity-coverage": "^0.5.11"
  },
  "scripts": {
    "clean": "./node_modules/.bin/rimraf -rf ./coverage.json ./build ./coverage ./coverageEnv ./.nyc-output",
    "lint:js": "./node_modules/.bin/eslint .",
    "lint:js:fix": "./node_modules/.bin/eslint . --fix",
    "lint:sol": "./node_modules/.bin/solium -d .",
    "lint:sol:fix": "./node_modules/.bin/solium -d . --fix",
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:fix": "npm run lint:js:fix && npm run lint:sol:fix",
    "test": "./node_modules/.bin/truffle test",
    "test:doorbell": "NODE_ENV=TEST ./node_modules/.bin/mocha ./extractPublicKey --exclude docs/ --trace-warnings --exit --colors --recursive --reporter spec",
    "deploy:s3": "aws s3 sync build/contracts/ s3://paulrberg/aztec/contracts --delete"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AztecProtocol/AZTEC.git"
  },
  "author": "",
