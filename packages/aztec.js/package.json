{
  "name": "aztec.js",
  "description": "AZTEC cryptography library",
  "version": "0.0.0-semantically-released",
  "author": "AZTEC",
  "bugs": {
    "url": "https://github.com/AztecProtocol/AZTEC/issues"
  },
  "dependencies": {
    "bn.js": "^4.11.8",
    "cross-fetch": "^3.0.2",
    "elliptic": "^6.4.1",
    "ethereumjs-abi": "^0.6.7",
    "ethereumjs-util": "^6.1.0",
    "mcl-wasm": "^0.3.2",
    "web3-eth-abi": "1.0.0-beta.50",
    "web3-utils": "1.0.0-beta.50",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "devDependencies": {
    "@aztec/dev-utils": "0.0.0-semantically-released",
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-loader": "^8.0.5",
    "braintree-jsdoc-template": "^3.3.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^2.0.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "fixpack": "^2.3.1",
    "jsdoc": "^3.5.5",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "push-dir": "^0.4.1",
    "shx": "^0.3.2",
    "sinon": "^7.2.3",
    "wasm-loader": "^1.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">=8.3",
    "yarn": "1.15.2"
  },
  "files": [
    "/lib"
  ],
  "homepage": "https://github.com/AztecProtocol/AZTEC/tree/master/packages/aztec.js#readme",
  "keywords": [
    "aztec",
    "blockchain",
    "confidentiality",
    "cryptography",
    "ethereum",
    "privacy",
    "protocol"
  ],
  "license": "LGPL-3.0",
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AztecProtocol/AZTEC.git"
  },
  "scripts": {
    "build": "yarn clean && babel --copy-files --out-dir ./lib --root-mode upward ./src",
    "build:docs": "jsdoc --configure ./.jsdoc.json",
    "clean": "shx rm -rf ./.nyc-output ./build ./coverage ./coverageEnv ./coverage.json ./docs ./lib || true",
    "coverage": "nyc mocha ./test --bail --trace-warnings --exit --colors --recursive --reporter spec",
    "lint": "eslint --ignore-path ../../.eslintignore .",
    "publish:docs": "yarn build:docs && push-dir --dir ./docs --branch gh-pages --remote origin --cleanup",
    "test": "mocha ./test --bail --colors --exit --recursive --reporter spec --timeout 0 --trace-warnings",
    "watch": "yarn build --watch",
    "webpack:dev": "webpack-dev-server --open --config ./webpack.dev.js",
    "webpack:prod": "webpack --config ./webpack.prod.js"
  }
}
