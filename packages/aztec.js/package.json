{
  "name": "aztec.js",
  "description": "AZTEC cryptography library",
  "version": "0.6.0",
  "author": "AZTEC",
  "bugs": {
    "url": "https://github.com/AztecProtocol/AZTEC/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bn.js": "^4.11.8",
    "elliptic": "^6.4.1",
    "ethereumjs-abi": "^0.6.7",
    "ethereumjs-util": "^6.1.0",
    "hash.js": "^1.1.7",
    "web3-eth-abi": "1.0.0-beta.50",
    "web3-utils": "1.0.0-beta.50",
    "websocket": "^1.0.28"
  },
  "devDependencies": {
    "@aztec/dev-utils": "^1.6.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.0",
    "braintree-jsdoc-template": "^3.3.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "fixpack": "^2.3.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "push-dir": "^0.4.1",
    "shx": "^0.3.2",
    "sinon": "^7.2.3"
  },
  "engines": {
    "node": ">=6.12"
  },
  "files": [
    "/lib"
  ],
  "homepage": "https://github.com/AztecProtocol/AZTEC#readme",
  "keywords": [
    "aztec",
    "blockchain",
    "confidentiality",
    "cryptography",
    "ethereum",
    "privacy",
    "protocol"
  ],
  "license": "LGPL-3.0",
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AztecProtocol/AZTEC.git"
  },
  "scripts": {
    "build": "yarn clean && cross-env BABEL_ENV=production babel ./src --copy-files --out-dir ./lib",
    "build:docs": "jsdoc -c ./.jsdoc.json",
    "clean": "shx rm -rf ./lib ./.nyc-output ./build ./coverage ./coverageEnv ./coverage.json ./lib || true",
    "coverage": "cross-env NODE_ENV=TEST nyc mocha ./test --trace-warnings --exit --colors --recursive --reporter spec",
    "lint": "eslint --ignore-path ../../.eslintignore .",
    "publish:docs": "yarn build:docs && push-dir --dir ./docs --branch gh-pages --remote origin --cleanup",
    "test": "cross-env NODE_ENV=TEST mocha ./test --trace-warnings --exit --colors --recursive --reporter spec"
  }
}
